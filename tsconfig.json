{
  "compilerOptions": {
    "jsx": "preserve",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "lib": ["dom", "es2019", "WebWorker"],
    "module": "ESNext",
    "target": "ES2020"
  },
  "exclude": ["node_modules"]
}
